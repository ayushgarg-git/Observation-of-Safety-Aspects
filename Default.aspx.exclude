    <script language="javascript" type="text/javascript">
        function fixedlength(textboxID, keyEvent) {
            if (textboxID.value.length > 1) {
                textboxID.value = textboxID.value.replace(/[^\d]+/g, '');
                return true;
            }
            else
                return false;
        }
    </script>
